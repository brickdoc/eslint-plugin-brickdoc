{
  "name": "@pleisto/eslint-plugin",
  "version": "2.2.2",
  "description": "ESLint plugin and custom rules for Pleisto Pte Ltd",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pleisto/eslint-plugin-pleisto.git"
  },
  "bugs": {
    "url": "https://github.com/pleisto/eslint-plugin-pleisto"
  },
  "license": "Apache-2.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint:type": "tsc --noEmit",
    "lint:eslint": "eslint src",
    "lint": "run lint:type && run lint:eslint",
    "typegen": "tsx tools/typegen.ts",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "build": "run typegen && tsup --format esm,cjs --clean --dts"
  },
  "dependencies": {
    "@eslint-react/tools": "^1.12.1",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "eslint-config-flat-gitignore": "^0.1.8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-flat-config-utils": "^0.3.1",
    "eslint-plugin-import-x": "^4.1.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^3.2.0",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-compiler": "0.0.0-experimental-72f06b2-20240822",
    "eslint-plugin-react-hooks": "^5.1.0-rc-b57d2823-20240822",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-unused-imports": "^4.1.3",
    "eslint-plugin-vitest": "^0.5.4"
  },
  "peerDependencies": {
    "eslint": ">=9.6.0",
    "typescript": ">=4.9.5"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^0.5.4",
    "@types/node": "^22.5.0",
    "eslint": "^9.9.1",
    "eslint-typegen": "^0.3.1",
    "tsup": "^8.2.4",
    "tsx": "^4.18.0",
    "typescript": "^5.5.4"
  },
  "packageManager": "yarn@4.3.1",
  "volta": {
    "node": "22.7.0"
  }
}
